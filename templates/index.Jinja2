    <link href=" {{ url_for('static',filename='assets/plugins/bower_components/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.css') }}"
          rel="stylesheet">
    <link href=" {{ url_for('static',filename='assets/css/animate.css') }}" rel="stylesheet">
